{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": [2, "global"],
    "no-underscore-dangle": 0,
    "quotes": [
      2,
      "single"
    ],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
  },
  "plugins" : [
    "react"
  ]
}